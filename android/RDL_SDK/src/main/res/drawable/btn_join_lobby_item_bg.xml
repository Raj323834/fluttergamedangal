<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape>
            <gradient android:angle="90" android:startColor="#212E66" android:endColor="#30112D"/>


            <corners android:radius="7dp" />
        </shape>
    </item>
    <item>
        <shape>
            <gradient android:angle="270" android:startColor="#212E66" android:endColor="#30112D"/>
            <corners android:radius="7dp" />
        </shape>

    </item>
</selector>
